<div class="form-body">
	<div class="form-group form-md-line-input">
		<label class="col-md-2 control-label" for="form_control_1">Name</label>
		<div class="col-md-10">
      <%= text_field_tag 'service[name]', nil, class: "form-control", placeholder: 'Enter Name', id: 'form_control_1' %>			
      <div class="form-control-focus"></div>
		</div>
	</div>
	<div class="form-group form-md-line-input">
		<label class="col-md-2 control-label" for="form_control_1">Category</label>
		<div class="col-md-10">
      <%= select_tag 'service[category_id]', options_for_select(category_options), {class: "form-control input-xlarge select2me", "data-placeholder" => "Select Category..."} %>
      <div class="form-control-focus"></div>
		</div>
	</div>
	<div class="form-group form-md-line-input">
		<label class="col-md-2 control-label" for="form_control_1">Contact Number</label>
		<div class="col-md-10">
      <%= text_field_tag 'service[contact_number]', nil, class: "form-control", placeholder: 'Enter Contact Number', id: 'form_control_1' %>			
      <div class="form-control-focus"></div>
		</div>
	</div>
	<div class="form-group form-md-line-input">
		<label class="col-md-2 control-label" for="form_control_1">Description</label>
		<div class="col-md-10">
      <%= text_area_tag 'service[description]', nil, class: "form-control autosizeme", placeholder: 'Enter Description', id: 'form_control_1' %>			
      <div class="form-control-focus"></div>
		</div>
	</div>
	<div class="form-group form-md-line-input">
		<label class="col-md-2 control-label" for="form_control_1">Keyword</label>
		<div class="col-md-10">
      <%= text_field_tag 'service[keyword]', nil, class: "form-control", placeholder: 'Enter Keyword', id: 'form_control_1' %>			
      <div class="form-control-focus"></div>
		</div>
	</div>
	<div class="form-group form-md-line-input">
		<label class="col-md-2 control-label" for="form_control_1">Street1</label>
		<div class="col-md-10">
      <%= text_field_tag "service_address[street1]", nil, class: "form-control", placeholder: ' Enter Street1', id: 'form_control_1' %>
      <div class="form-control-focus"></div>
		</div>
	</div>
	<div class="form-group form-md-line-input">
		<label class="col-md-2 control-label" for="form_control_1">Street2</label>
		<div class="col-md-10">
      <%= text_field_tag "service_address[street2]", nil, class: "form-control", placeholder: ' Enter Street2', id: 'form_control_1' %>
      <div class="form-control-focus"></div>
		</div>
	</div>
	<div class="form-group form-md-line-input">
		<label class="col-md-2 control-label" for="form_control_1">City</label>
		<div class="col-md-10">
      <%= text_field_tag "service_address[city]", nil, class: "form-control", placeholder: ' Enter City', id: 'form_control_1' %>
      <div class="form-control-focus"></div>
		</div>
	</div>
	<div class="form-group form-md-line-input">
		<label class="col-md-2 control-label" for="form_control_1">Region</label>
		<div class="col-md-10">
      <%= text_field_tag "service_address[region]", nil, class: "form-control", placeholder: ' Enter Region', id: 'form_control_1' %>
      <div class="form-control-focus"></div>
		</div>
	</div>
	<div class="form-group form-md-line-input">
		<label class="col-md-2 control-label" for="form_control_1">Postal Code</label>
		<div class="col-md-10">
      <%= text_field_tag "service_address[postal_code]", nil, class: "form-control", placeholder: ' Enter Postal Code', id: 'form_control_1' %>
      <div class="form-control-focus"></div>
		</div>
	</div>
<!--
	<div class="form-group form-md-line-input">
		<label class="col-md-2 control-label" for="form_control_1">Country</label>
		<div class="col-md-10">
      <%= select_tag "service_address[country]", options_for_select(country_options), {class: "form-control input-xlarge select2me", "data-placeholder" => "Select Country..."} %>
      <div class="form-control-focus"></div>
		</div>
	</div>
-->
	<div class="form-group form-md-line-input">
	  <label class="col-md-2 control-label" for="form_control_1">Tags</label>
		<div class="col-md-10">
      <%= select_tag 'service[tag_ids]', tagging_options, {class: "form-control input-xlarge select2me", multiple: true, id: 'form_control_1', "data-placeholder" => "Select Tags..."} %>
			<div class="form-control-focus"></div>
		</div>
	</div>
</div>
